    "@types/eslint": "^9.6.1",
    "@types/file-entry-cache": "^5.0.4",
    "@types/safe-regex": "^1.1.6",
    "@types/yargs-parser": "^21.0.3",
    "@typescript-eslint/utils": "^7.9.0",
    "eslint": "^8.57.0",
    "file-entry-cache": "^11.1.0",
    "globby": "^13.2.2",
    "patch-package": "^8.0.0",
    "rimraf": "^5.0.5",
    "tsx": "^4.20.6",
    "typescript": "5.5.4",
    "vitest": "^1.3.1",
    "yargs-parser": "^22.0.0"
