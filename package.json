{
  "name": "eslint-plugin-perf-fiscal",
  "version": "0.3.1",
  "description": "ESLint plugin focused on catching performance pitfalls and anti-patterns.",
  "keywords": [
    "eslint",
    "eslintplugin",
    "performance",
    "react",
    "security"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruidosujeira/perf-linter.git"
  },
  "bugs": {
